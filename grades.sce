grade = input("Enter your grade : ")
if grade>90 then
    disp("O")
 elseif grade>=80 & grade<=89 then
    disp("E")
elseif grade>=70 & grade<=79 then
    disp("A")
elseif grade>=60 & grade<=69 then
    disp("B")
elseif grade>=50 & grade<=59 then
    disp("Poor")
elseif grade>=40 & grade<=49 then
    disp("Very Poor")
    else
        disp("F")
end